{
  "name": "express-auto-routing",
  "version": "1.0.0",
  "description": "Auto generated routes based of a specified folder",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "express"
  ],
  "files": [
    "lib/**/*"
  ],
  "scope": "@gijslaarman",
  "author": "gijslaarman",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/gijslaarman/express-auto-routing"
  },
  "bugs": {
    "url": "https://github.com/gijslaarman/express-auto-routing/issues"
  },
  "homepage": "https://github.com/gijslaarman/express-auto-routing#readme",
  "dependencies": {
    "express": "^4.17.1",
    "glob": "^7.1.6"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/glob": "^7.1.3",
    "@types/node": "^14.14.37",
    "prettier": "^2.2.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.2.4"
  }
}
